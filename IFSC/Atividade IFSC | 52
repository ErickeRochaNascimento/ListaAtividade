#include <stdio.h>

int main() {
    float num1, num2;

    printf("Digite o primeiro número: ");
    scanf("%f", &num1);

    printf("Digite o segundo número: ");
    scanf("%f", &num2);

    if (num1 == num2) {
        printf("Os números são iguais.\n");
    } else {
        printf("Os números são diferentes.\n");

        if (num1 > num2) {
            printf("Maior: %f\n", num1);
            printf("Menor: %f\n", num2);
        } else {
            printf("Maior: %f\n", num2);
            printf("Menor: %f\n", num1);
        }
    }

    return 0;
}
