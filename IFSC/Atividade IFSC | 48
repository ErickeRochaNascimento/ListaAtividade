#include <stdio.h>

int main() {
    int a, b, c, menor;

    printf("Digite três números inteiros distintos:\n");
    scanf("%d %d %d", &a, &b, &c);

    menor = a;

    if (b < menor) {
        menor = b;
    }
    if (c < menor) {
        menor = c;
    }

    printf("O menor número é: %d\n", menor);

    return 0;
}
