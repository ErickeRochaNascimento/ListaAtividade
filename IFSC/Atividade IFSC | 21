#include <stdio.h>

int main() {
    int a,b,soma,resultado;
    
    printf("Calcule ao Quadrado");
    printf("\nDigite um numero: ");
    scanf("%d",&a);
    printf("Digite um numero: ");
    scanf("%d",&b);
    
    resultado = (a * a) + (b + b);

    printf("A soma do quadrado de %d e %d Ã©: %d\n", a, b, resultado);

    return 0;
}