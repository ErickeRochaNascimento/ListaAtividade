#include <stdio.h>

int main() {
    int a, b, i, j;

    for (i = 0; i < 5; i++) {
        printf("Digite os valores de a e b (com a < b): ");
        scanf("%d %d", &a, &b);

        if (a < b && a > 0 && b > 0) {
            for (j = a; j <= b; j++) {
                if (j % 2 == 0) {
                    printf("%d\n", j);
                }
            }
        } else {
            printf("Par invÃ¡lido. Certifique-se de que a < b e ambos positivos.\n");
        }
    }

    return 0;
}